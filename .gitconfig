[user]
	name = toejough
	email = toejough@gmail.com
[color]
	ui = always
[core]
	ignoresFile = /Users/joe/.gitignore
	excludesFile = /Users/joe/.gitignore
	editor = vim
[credential]
	helper = osxkeychain
[alias]
	forget = rm --cached
	prune-all = "!git remote prune origin; git branch --merged | egrep -v '^\\s*\\*?\\s*master$' | xargs -L1 git branch -d"
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[push]
	default = simple
[commit]
	template = /Users/joe/.settings/git-commit-template.txt
[help]
	autocorrect = 50
[pull]
	ff = only
[rebase]
	autosquash = true
[fetch]
	prune = true
[column]
	ui = auto dense column

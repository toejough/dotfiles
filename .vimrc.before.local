" Use powerline patched fonts
let g:airline_powerline_fonts=1

" List only the plugin groups that get used
let g:spf13_bundle_groups=['general', 'programming', 'python', 'javascript', 'ruby', 'html', 'misc']

" Disable OmniComplete
" - it screws up the arrow keys (https://github.com/spf13/spf13-vim/issues/567)
let g:spf13_no_omni_complete = 1
